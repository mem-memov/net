all: count boat direction gap link mesh net node place space star

boat:
	$(MAKE) -C Boat
count:
	$(MAKE) -C Count
direction:
	$(MAKE) -C Direction
gap:
	$(MAKE) -C Gap
link:
	$(MAKE) -C Link
mesh:
	$(MAKE) -C Mesh
net:
	$(MAKE) -C Net
node:
	$(MAKE) -C Node
place:
	$(MAKE) -C Place
space:
	$(MAKE) -C Space
star:
	$(MAKE) -C Star
telescope:
	$(MAKE) -C Telescope
